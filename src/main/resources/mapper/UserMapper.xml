<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.white.mapper.UserMapper">
    <resultMap id="userOrderMap" type="com.white.beans.User">


    </resultMap>
    <select id="selectOrderByUsername" parameterType="string" resultMap="userOrderMap">
        select  *  from orders o left join users u on u.user_id = o.user_id where username = #{username}
    </select>
</mapper>
